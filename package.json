{
  "name": "@xuyanfeng/cc-ui",
  "version": "0.2.12",
  "main": "dist/ccui.umd.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "yarnInstallForce": "yarn install --force",
    "demo-serve": "vue-cli-service serve",
    "demo-build": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "types": "vue-tsc -p tsconfig.declaration.json",
    "lib-dev": "vue-cli-service build --mode development --target lib --name ccui --dest dist --entry packages/index.ts",
    "lib-prd": "vue-cli-service build --mode production --target lib --name ccui --dest dist --entry packages/index.ts",
    "yalc-dev": "npm run lib-dev && npm run types && yalc push",
    "yalc-prd": "npm run lib-prd && npm run types && yalc push"
  },
  "files": [
    "dist",
    "iconfont",
    "types"
  ],
  "dependencies": {
    "@popperjs/core": "2.11.6",
    "@types/lodash": "4.14.185",
    "@types/mousetrap": "1.6.12",
    "@types/shortid": "0.0.30",
    "@typescript-eslint/parser": "5.38.1",
    "chroma-js": "2.1.2",
    "core-js": "3.6.5",
    "less-loader": "6.2.0",
    "lodash": "4.17.21",
    "mousetrap": "1.6.5",
    "shortid": "2.2.16",
    "tiny-emitter": "2.1.0",
    "tinycolor2": "1.4.2",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "vue": "3.2.26",
    "vue-tsc": "0.40.13",
    "vue3-resize-directive": "2.0.3",
    "xmlhttprequest": "1.8.0",
    "pinia": "2.0.33"
  },
  "devDependencies": {
    "@types/chroma-js": "2.1.3",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-typescript": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "3.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "8.2.0",
    "lint-staged": "9.5.0",
    "typescript": "4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tidys/cc-ui.git"
